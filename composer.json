{
  "name": "davidhirtz/yii2-cms",
  "type": "yii2-extension",
  "description": "Yii2 CMS Module",
  "homepage": "https://www.davidhirtz.com/",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "davidhirtz/yii2-skeleton": "*",
    "davidhirtz/yii2-media": "*"
  },
  "autoload": {
    "psr-4": {
      "davidhirtz\\yii2\\cms\\": ""
    }
  },
  "extra": {
    "bootstrap": "davidhirtz\\yii2\\cms\\composer\\Bootstrap"
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:davidhirtz/yii2-skeleton.git"
    },
    {
      "type": "git",
      "url": "git@github.com:davidhirtz/yii2-media.git"
    }
  ]
}